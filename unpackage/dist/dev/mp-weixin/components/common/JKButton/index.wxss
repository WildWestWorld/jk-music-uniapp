/* components/common/JKButton/index.wxss */
.JK-button .primary {
  border-radius: 20rpx;
  position: relative;
  display: inline-flex;
  align-items: center;
  justify-content: center;
  box-sizing: border-box;
  padding: 0;
  text-align: center;
  vertical-align: middle;
  -webkit-appearance: none;
  -webkit-text-size-adjust: 100%;
  height: 88rpx;
  background: #0041c4;
  color: white;
  /* reset weapp default border*/
}
.JK-button .primary::before {
  position: absolute;
  top: 50%;
  left: 50%;
  width: 100%;
  height: 100%;
  border: inherit;
  border-radius: inherit;
  /* inherit parent's border radius */
  -webkit-transform: translate(-50%, -50%);
          transform: translate(-50%, -50%);
  opacity: 0;
  content: ' ';
  background-color: black;
  border-color: black;
}
.JK-button .primary::after {
  border-width: 0;
}
.JK-button .svg {
  background-color: #e5ecf9;
  border-radius: 20rpx;
  position: relative;
  display: inline-flex;
  align-items: center;
  justify-content: center;
  box-sizing: border-box;
  padding: 0;
  text-align: center;
  vertical-align: middle;
  -webkit-appearance: none;
  -webkit-text-size-adjust: 100%;
  height: 88rpx;
  color: white;
  /* reset weapp default border*/
}
.JK-button .svg::before {
  position: absolute;
  top: 50%;
  left: 50%;
  width: 100%;
  height: 100%;
  border: inherit;
  border-radius: inherit;
  /* inherit parent's border radius */
  -webkit-transform: translate(-50%, -50%);
          transform: translate(-50%, -50%);
  opacity: 0;
  content: ' ';
  background-color: black;
  border-color: black;
}
.JK-button .svg::after {
  border-width: 0;
}
.JK-button .icon {
  width: 48rpx;
  height: 48rpx;
}
.hover-primary {
  background: #0041c4;
  opacity: 0.8;
}
.hover-svg {
  background: #7A869A;
  opacity: 0.8;
}

