.header-container {
  padding: 0 50rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 40rpx;
}
.header-container .title {
  font-size: 44rpx;
  color: #002251;
  line-height: 52rpx;
}
.header-container .more {
  font-style: 28rpx;
  color: #0041c4;
  line-height: 40rpx;
}
